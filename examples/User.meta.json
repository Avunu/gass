{
  "$schema": "../types/entry-meta.schema.json",
  "sheetId": 123456789,
  "headerRow": 1,
  "dataStartColumn": 1,
  "dataEndColumn": 6,
  "columns": ["id", "name", "email", "phone", "status", "createdAt"],
  "defaultSort": [
    {
      "column": "name",
      "ascending": true
    }
  ],
  "fields": {
    "id": {
      "type": "string",
      "required": true,
      "description": "Unique identifier for the user"
    },
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1,
      "maxLength": 100,
      "description": "Full name of the user"
    },
    "email": {
      "type": "string",
      "required": true,
      "format": "email",
      "description": "Email address of the user"
    },
    "phone": {
      "type": "string",
      "pattern": "^[0-9]{3}-[0-9]{3}-[0-9]{4}$",
      "description": "Phone number in format XXX-XXX-XXXX"
    },
    "status": {
      "type": "string",
      "enum": ["active", "inactive", "pending"],
      "required": true,
      "description": "Current status of the user"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "When the user was created"
    }
  }
}
